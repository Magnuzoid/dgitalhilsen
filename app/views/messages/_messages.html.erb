<% obj.each do |message|%>
  <%# if message.user_id == current_user.id %>
  <li>
    <%= message.id %>
    <%= message.user_id %>
    <%= link_to "show", message_path(message) %>
    <%= link_to "delete", message_path(message),
                          :method => :DELETE,
                          :data => { :confirm => "Are you sure?" },
                          :class => "delete-message"
                          %>
  </li>
  <%# end %>

<% end %>
